class noithanh gia hạn chuyenxe{
sokm đôi riêng tư;
pritave string sotuyen;
// cac ham khoi tao...
//cac ham get/set...
chuỗi công khai tostring()
     return"chuyen xe noi thanh" +
               super.tostring()+
               "\nso tuyen :" + this.sotuyen +
               "\nsokm:"+ this.sokm
               "\ndoanhthu :" + this.doanhthu
  }
}
class ngoaithanh extends chuyenxe{
  chuỗi riêng biệt;
  pritave int songay;
  public string tostring();
     return "\nhuyen xe ngoai thanh" +
              super.tostring() +
              "\nnoiden:"+this.noiden
              "\nsongay:"+this.songay
              "\ndoanhthu: " +this.doanhthu
  }
}
